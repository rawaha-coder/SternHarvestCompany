<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="665.0" prefWidth="476.0"
            stylesheets="@../../style.css"
            xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="harvest.ui.harvest.AddHarvestController">
   <children>
      <Label layoutX="110.0" layoutY="55.0" prefHeight="30.0" prefWidth="240.0" text="Harvest Date" />
      <DatePicker fx:id="fxHarvestDate" layoutX="110.0" layoutY="85.0" minWidth="150.0" prefHeight="30.0" prefWidth="240.0" />
      <Label layoutX="110.0" layoutY="128.0" prefHeight="30.0" prefWidth="240.0" text="Supplier" />
      <ChoiceBox fx:id="fxSupplierList" layoutX="110.0" layoutY="158.0" prefHeight="30.0" prefWidth="240.0" />
      <Label layoutX="110.0" layoutY="211.0" prefHeight="30.0" prefWidth="240.0" text="Farm" />
      <ChoiceBox fx:id="fxFarmList" layoutX="110.0" layoutY="241.0" prefHeight="30.0" prefWidth="240.0" />
      <Label layoutX="110.0" layoutY="296.0" prefHeight="30.0" prefWidth="240.0" text="Product" />
      <ChoiceBox fx:id="fxProductList" layoutX="110.0" layoutY="326.0" prefHeight="30.0" prefWidth="240.0" />
      <Label layoutX="110.0" layoutY="381.0" prefHeight="30.0" prefWidth="240.0" text="Code" />
      <ChoiceBox fx:id="fxProductCodeList" layoutX="110.0" layoutY="411.0" prefHeight="30.0" prefWidth="240.0" />
      <Button fx:id="fxSaveButton" layoutX="90.0" layoutY="514.0" mnemonicParsing="false" onAction="#handleSaveButton" prefHeight="30.0" prefWidth="120.0" text="Save" />
      <Button fx:id="fxClearButton" layoutX="244.0" layoutY="514.0" mnemonicParsing="false" onAction="#handleClearButton" prefHeight="30.0" prefWidth="120.0" text="Clear" />
      <Button fx:id="fxCloseButton" layoutX="110.0" layoutY="582.0" mnemonicParsing="false" onAction="#handleCloseButton" prefHeight="30.0" prefWidth="240.0" text="Close" />
   </children>
</AnchorPane>
