<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="displayHarvestWorkUI"
            prefHeight="700.0" prefWidth="1400.0" stylesheets="@../../style.css"
            xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="harvest.ui.harvest.GetHarvestWork">

    <TableView fx:id="fxDisplayHarvestWorkTable" editable="true" layoutX="28.0" layoutY="14.0" prefHeight="706.0" prefWidth="1024.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
        <columns>
            <TableColumn fx:id="fxEmployeeNameColumn" minWidth="130.0" prefWidth="180.0" text="Employee" />
            <TableColumn fx:id="fxAllQuantityColumn" minWidth="80.0" prefWidth="120.0" text="All QNT" />
            <TableColumn fx:id="fxBadQualityColumn" minWidth="80.0" prefWidth="120.0" text="Bad QLT" />
            <TableColumn fx:id="fxGoodQualityColumn" minWidth="80.0" prefWidth="120.0" text="Good QLT" />
            <TableColumn fx:id="fxProductPriceColumn" minWidth="80.0" prefWidth="120.0" text="Price" />
            <TableColumn fx:id="fxTransportColumn" minWidth="80.0" prefWidth="100.0" text="Transport" />
            <TableColumn fx:id="fxCreditColumn" minWidth="80.0" prefWidth="100.0" text="Credit" />
            <TableColumn fx:id="fxNetAmountColumn" minWidth="80.0" prefWidth="100.0" text="Net" />
            <TableColumn fx:id="fxRemarqueColumn" minWidth="140.0" prefWidth="220.0" text="Remarque" />
        </columns>
    </TableView>

</AnchorPane>
